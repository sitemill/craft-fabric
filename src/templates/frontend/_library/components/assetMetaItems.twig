{% set metaItems = craft.library.settings.fileKinds[asset.kind].metaItems is defined ? craft.library.settings.fileKinds[asset.kind].metaItems : craft.library.settings.defaultMetaItems  %}

{% for metaItem in metaItems  %}
    {% set field = craft.fields.fieldByHandle(metaItem.fieldHandle) %}
    {% include '_library/components/metaItems/' ~ metaItem.type with field %}
{% endfor %}

