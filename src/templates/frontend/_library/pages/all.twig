{% set pageTitle = 'All Entries' %}
{% extends '_library/layouts/page.twig' %}

{% block pageHeader %}
    {% set pageTitle = keywords ? "Searching for '" ~ keywords ~ "'" : 'All Entries' %}
    {{ parent() }}
{% endblock %}

{% block pageMain %}
    {% if craft.library.settings.assetsSource %}
        {% include '_library/components/query/assets.twig' %}
    {% endif %}

    {% if craft.library.settings.entrySources|length %}
        {% include '_library/components/query/entries.twig' %}
    {% endif %}
{% endblock %}