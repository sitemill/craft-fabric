
{% set section = craft.sections.sectionByHandle(section|camel) %}
{% set entries = craft.entries.section(section).all() %}

{% extends '_library/layouts/app.twig' %}

{% block main %}
    {{ dump(section) }}
    {% set pageTitle = section.name  %}
    {% include '_library/components/pageHeader' %}

    <div class="entryListing">
        {% for entry in entries %}
            <a href="{{ url(section.handle ~ '/entry/' ~ entry.id ~ '/' ~ entry.slug) }}">
            {{ entry.title }}
            </a><br>
        {% endfor %}
    </div>
{% endblock %}