{
  "name": "sitemill/craft-fabric",
  "description": "Fabric is framework for your Craft content.",
  "type": "craft-plugin",
  "version": "1.0.0-alpha",
  "keywords": [
    "craft",
    "cms",
    "craftcms",
    "craft-plugin",
    "fabric"
  ],
  "support": {
    "docs": "https://github.com/sitemill/craft-fabric/blob/master/README.md",
    "issues": "https://github.com/sitemill/craft-fabric/issues"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "SiteMill",
      "homepage": "https://sitemill.co"
    }
  ],
  "require": {
    "craftcms/cms": "^3.5.0"
  },
  "require-dev": {
    "craftcms/cms": "^3.5.0",
    "codeception/codeception": "^3.0",
    "vlucas/phpdotenv": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "sitemill\\fabric\\": "src/"
    }
  },
  "extra": {
    "name": "Fabric",
    "handle": "fabric",
    "developer": "SiteMill",
    "developerUrl": "https://sitemill.co",
    "documentationUrl": "https://github.com/sitemill/craft-fabric/blob/master/README.md",
    "changelogUrl": "https://raw.githubusercontent.com/craft-sitemill/fabric/master/CHANGELOG.md",
    "components": {
      "share": "sitemill\\fabric\\services\\Share"
    },
    "class": "sitemill\\fabric\\Fabric"
  }
}
